@model PRC.Models.ViewModels.SlideshowsViewModel

@{
    ViewData["Title"] = "Create A New Slide";
    Layout = "_LayoutAdmin";
}

<h4>@ViewData["Title"]</h4> | <a href="/admin/newsletters">Back to Slides</a>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="File"></label>
                <div class="input-group">
                    <input asp-for="File" class="form-control" type="file" id="upload" />
                </div>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
